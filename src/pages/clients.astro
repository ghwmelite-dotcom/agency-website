---
import BaseLayout from '@/layouts/BaseLayout.astro';
import Header from '@/components/Header.astro';
import Footer from '@/components/Footer.astro';
import Clients from '@/components/Clients.astro';
import { SITE_CONFIG } from '@/config';

export const prerender = false;
---

<BaseLayout
  title={`Our Clients - ${SITE_CONFIG.name}`}
  description="Trusted by industry leaders across various sectors. Discover the companies we've partnered with to deliver exceptional digital solutions that drive growth and innovation."
>
  <Header />
  <main>
    <Clients />
  </main>
  <Footer />
</BaseLayout>
